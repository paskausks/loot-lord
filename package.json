{
  "name": "loot-lord",
  "version": "0.2.0",
  "description": "Yet another Discord bot",
  "main": "dist/index.js",
  "private": true,
  "devDependencies": {
    "@types/jest": "^25.2.2",
    "@types/node": "^13.13.6",
    "@types/signale": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "concurrently": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-import-resolver-node": "^0.3.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.0",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.2"
  },
  "scripts": {
    "build": "tsc && node dist/utils/write-build-info.js",
    "build:watch": "concurrently -p \"[{name}]\" -n \"TSC,Node\" \"npm run build:watch:tsc\" \"npm run build:watch:node\"",
    "build:watch:tsc": "tsc -w",
    "build:watch:node": "nodemon --watch dist dist/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "start": "node dist/index.js",
    "test": "jest",
    "knex": "knex",
    "migrate": "knex migrate:latest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^11.6.4",
    "dotenv": "^8.2.0",
    "knex": "^0.20.15",
    "moment": "^2.25.3",
    "rxjs": "^6.5.5",
    "signale": "^1.4.0",
    "sqlite3": "^4.2.0"
  },
  "signale": {
    "displayLabel": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  }
}
